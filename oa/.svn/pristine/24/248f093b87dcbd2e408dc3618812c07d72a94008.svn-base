package com.lh.it.resource.core.vo;

import com.lh.it.resource.core.entity.StateType;
import com.lh.it.resource.security.entity.main.Organization;

public class EmployeeVo {

	private String name;

	private Organization orga;

	private StateType stateType;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Organization getOrga() {
		return orga;
	}

	public void setOrga(Organization orga) {
		this.orga = orga;
	}

	public StateType getStateType() {
		return stateType;
	}

	public void setStateType(StateType stateType) {
		this.stateType = stateType;
	}

}