<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ include file="/WEB-INF/views/include.inc.jsp"%>

<div class="pageContent">
		<div class="pageFormContent" layoutH="58">
	         <div class="unit">
				<label>区域名称：</label>
				<input type="text" size="30" name="regionName" disabled="disabled" value="${region.regionName}" />
			</div>
			<div class="unit">
				<label>短名称：</label>
				<input type="text" name="shortName" size="30" disabled="disabled" maxlength="20" class="required" value="${region.shortName}"/>
			</div>
			<div class="unit">
				<label >邮政编码：</label>
				<input type="text" name="zipcode" size="30" disabled="disabled" maxlength="20" class="required" value="${region.zipcode}"/>
			</div>
			<div class="unit">
				<label >区域拼音：</label>
				<input type="text" name="pinyin" size="30" disabled="disabled" maxlength="20" class="required" value="${region.pinyin}"/>
			</div>
			<div class="unit">
				<label >长：</label>
				<input type="text" name="lng" size="30" disabled="disabled" maxlength="20" class="required" value="${region.lng}"/>
			</div>
			<div class="unit">
				<label >短：</label>
				<input type="text" name="lat" size="30" disabled="disabled" maxlength="20" class="required" value="${region.lat}"/>
			</div>
			<div class="unit">
				<label >父类字符串全称：</label>
				<input type="text" name="position" size="30" disabled="disabled" maxlength="20" class="required" value="${region.position}"/>
			</div>
		</div>
		<div class="formBar">
			<ul>
				<li><div class="button"><div class="buttonContent"><button type="button" class="close">关闭</button></div></div></li>
			</ul>
		</div>
</div>
